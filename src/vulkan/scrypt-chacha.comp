				tmp[2] += tmp[3]; t = tmp[1] ^ tmp[2]; tmp[1] = ROTL32(t, ROTATE_7);
		
				tmp[0]      += tmp[1].yzwx; t = tmp[3].wxyz ^ tmp[0];      tmp[3].wxyz = ROTL32(t, ROTATE_16);
				tmp[2].zwxy += tmp[3].wxyz; t = tmp[1].yzwx ^ tmp[2].zwxy; tmp[1].yzwx = ROTL32(t, ROTATE_12);
