//		chacha_core(X + 4);
		tmp[0] = X[4];
		tmp[1] = X[5];
		tmp[2] = X[6];
		tmp[3] = X[7];

		for (rounds = 0; rounds < 4; rounds ++) {
			tmp[0] += tmp[1]; t = tmp[3] ^ tmp[0]; tmp[3] = ROTL32(t, ROTATE_16);
