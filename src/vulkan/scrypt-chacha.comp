#define ROTL64(x, y) rotate2(x, uint(y))
#define ROTL32(x, y) ((x << y) | (x >> (32-y)))

struct scrypt_hash_state {
	uvec4 state4[(SCRYPT_KECCAK_F + 127) / 128]; // 8 bytes of extra
